
@{
    Layout = null;
}

@model IEnumerable<BlogSite.DB.Etiket>


<div class="widget blog-tags">
    <h5>Etiketler</h5>
    <ul>

        @foreach (var item in Model)
        {
            <li><a href="/Etiket/Index/@item.ID" class="btn btn-default">@item.Adi (@item.Makales.Count)</a></li>
        }

    </ul>
</div>

